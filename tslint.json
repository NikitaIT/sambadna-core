{
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "linterOptions": {
        "exclude": [
            "config/**/*.js",
            "node_modules/"
        ]
    },
    "rules": {
        "jsx-no-lambda": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "jsx-boolean-value": false,
        "no-angle-bracket-type-assertion": false,
        "only-arrow-functions": false,
        "no-console": {
            "severity": "warning",
            "options": [
                "debug",
                "info",
                "log",
                "time",
                "timeEnd",
                "trace"
            ]
        }
    }
}